{
    "name": "state-space-tutorial Container",
    "build": {
      "context": "..",
      "dockerfile": "Dockerfile"
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-toolsai.jupyter"
        ]
      }
    },
    "mounts": [
      "source=vscode-extensions,target=/root/.vscode-server/extensions,type=volume"
    ]
  }